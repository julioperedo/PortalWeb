@{
    ViewData["Title"] = "Detalle de Clientes";
}
<div class="row">
    <div class="col">
        <h3 class="title">@ViewBag.Title</h3>
    </div>
    <div class="col text-right">
        @if (ViewBag.SeeMargin == "Y")
        {
            <a class="btn btn-secondary" role="button" href="#" id="action-show-all"><i class="fas fa-eye"></i></a>
        }
    </div>
</div>
<div class="row">
    <div class="col">
        <div id="filter-box" class="bg-light px-2 py-3 collapse show">
            <div class="row">
                <label for="Client" class="col-3 col-sm-2 col-lg-1 text-nowrap">Cliente</label>
                <div class="col-9 col-sm-8 col-lg col-xl">
                    <kendo-dropdownlist name="Client" option-label="Seleccione un Cliente..." datatextfield="name" datavaluefield="code" style="width: 100%;" ignore-case="true" filter="FilterType.Contains" on-change="onClientChanged">
                        <datasource>
                            <transport>
                                <read url="@Url.Action("GetClientsLocal", "CustomerDetail")" />
                            </transport>
                        </datasource>
                        <virtual enabled="true" item-height="26" value-mapper="clientMapper" />
                    </kendo-dropdownlist>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col pt-3 pb-2">
        <div class="card no-shadow bg-secondary text-white">
