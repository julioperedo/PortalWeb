@{
    ViewData["Title"] = "Configuraciones de la Alcancía Ganadora";
}
<div class="row">
    <div class="col">
        <h3 class="title">@ViewBag.Title</h3>
    </div>
</div>
<div class="row">
    <div class="row">
        <label for="products" class="col">Productos Asignados</label>
        <div class="col-md-11">
            <select multiple="multiple" id="products"></select>
        </div>
    </div>
</div>
@section Scripts {
    <script type="text/javascript">
        let urlImages = "@Url.Content("~/images/piggybank/prizes/")",
            urlNoImage = "@Url.Content("~/images/no-image.jpg")";
    </script>
    <environment include="Development">
        <script src="~/js/areas/marketing/piggybank-config.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/areas/marketing/piggybank-config.min.js" asp-append-version="true"></script>
    </environment>
}